{
  "name": "express-server",
  "version": "1.0.0",
  "description": "A Boilerplate NodeJS Express Server Template written in Typescript built for scale!",
  "main": "server.js",
  "scripts": {
    "start": "node code/src/server.js",
    "start:server": "pm2 start code/src/server.js --watch",
    "start:dev_1": "npx tsc && supervisor -e js,json,env,html,css code/index.js",
    "dev_": "tsc-watch --onSuccess \"nodemon code/src/server.js\"",
    "tsc": "tsc-watch",
    "node": "nodemon code/src/server.js",
    "dev": "npm-run-all --parallel tsc node"
  },
  "repository": {
    "type": "git",
    "url": "Express-Server"
  },
  "keywords": [
    "express",
    "server",
    "socketio",
    "redis",
    "mongodb",
    "mysql",
    "posgres",
    "server_events"
  ],
  "dependencies": {
    "@aws-sdk/client-ses": "^3.473.0",
    "@faker-js/faker": "^8.4.1",
    "@socket.io/redis-adapter": "^8.3.0",
    "@trpc/server": "^10.45.2",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "@types/socket.io": "^3.0.2",
    "backblaze-b2": "^1.7.0",
    "buffer-image-size": "^0.6.4",
    "compression": "^1.7.4",
    "cookie": "^0.6.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "imagemin": "7.0.0",
    "imagemin-jpeg-recompress": "^7.1.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-pngquant": "9.0.2",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.1.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.10.1",
    "nodemailer": "^6.9.7",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "pg": "^8.12.0",
    "redis": "^4.6.14",
    "request-stats": "^3.0.0",
    "sharp": "^0.33.4",
    "socket.io": "^4.6.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.5.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/backblaze-b2": "^1.5.6",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.8",
    "@types/uuid": "^9.0.8"
  },
  "author": "Day2DayJa",
